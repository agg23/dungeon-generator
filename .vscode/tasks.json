  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Generator",
      "type": "shell",
      "command": "java -jar ../millfork/target/scala-2.12/millfork.jar ./millfork/generator-main.mfk -i ./millfork/lib -o build/generator.nes -g -s -fsource-in-asm -t nes_small -fillegals -O4",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Build RNG",
      "type": "shell",
      "command": "java -jar ../millfork/target/scala-2.12/millfork.jar ./millfork/rng-main.mfk -o build/rng.nes -g -s -fsource-in-asm -t nes_small -fillegals -O4",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
